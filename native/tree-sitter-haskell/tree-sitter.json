{
  "grammars": [
    {
      "name": "haskell",
      "camelcase": "Haskell",
      "scope": "source.hs",
      "path": ".",
      "file-types": [
        "hs",
        "hs-boot"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "tags": [
        "queries/tags.scm"
      ],
      "injection-regex": "^(hs|haskell)$"
    },
    {
      "name": "hsc",
      "camelcase": "HaskellC",
      "scope": "source.hsc",
      "path": "hsc",
      "file-types": [
        "hsc"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "tags": [
        "queries/tags.scm"
      ],
      "injection-regex": "^(hs|haskell|hsc)$"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Haskell grammar for tree-sitter",
    "authors": [
      {
        "name": "Torsten Schmits"
      }
    ],
    "links": {
      "repository": "https://github.com/tek/tree-sitter-haskell"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
