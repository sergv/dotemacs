     ;; :red-aux-aux-aux-bg

     :red-aux-aux-aux-bg        (solarized-get-color 'red-2bg     palette)
     :orange-aux-aux-aux-bg     (solarized-get-color 'orange-2bg  palette)
     :yellow-aux-aux-aux-bg     (solarized-get-color 'yellow-2bg  palette)
     :green-aux-aux-aux-bg      (solarized-get-color 'green-2bg   palette)
