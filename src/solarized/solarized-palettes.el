(defsubst num->color (x)
  (format "#%06x" x))

(eval-when-compile
  (defun solarized-get-color (key xs)
    (declare (pure t) (side-effect-free nil))
    (aif (assq key xs)
        (progn
          (cl-assert (fixnump (cdr it))
                     "Color is not a number: %s" (car it))
          (cdr it))
      (error "Key not found: %s" key)))

  (defun make-solarized-palette (light? palette)
    (declare (pure t) (side-effect-free t))
    (make--solarized-palette
     :mode                      (if light? 'light 'dark)

     :background                (solarized-get-color (if light? 'base3 'base03) palette)
     :background-highlights     (solarized-get-color (if light? 'base2 'base02) palette)
     :secondary-content         (solarized-get-color (if light? 'base1 'base01) palette)
     :aux-mid                   (solarized-get-color (if light? 'base0 'base00) palette)
     :primary-content           (solarized-get-color (if light? 'base00 'base0) palette)
     :emphasized-content        (solarized-get-color (if light? 'base01 'base1) palette)
     :aux-high-1                (solarized-get-color (if light? 'base02 'base2) palette)
     :aux-high-2                (solarized-get-color (if light? 'base03 'base3) palette)

     :red                       (solarized-get-color 'red     palette)
     :orange                    (solarized-get-color 'orange  palette)
     :yellow                    (solarized-get-color 'yellow  palette)
     :green                     (solarized-get-color 'green   palette)
     :cyan                      (solarized-get-color 'cyan    palette)
     :blue                      (solarized-get-color 'blue    palette)
     :violet                    (solarized-get-color 'violet  palette)
     :magenta                   (solarized-get-color 'magenta palette)

     :red-aux-fg                (solarized-get-color (if light? 'red-d     'red-l)     palette)
     :orange-aux-fg             (solarized-get-color (if light? 'orange-d  'orange-l)  palette)
     :yellow-aux-fg             (solarized-get-color (if light? 'yellow-d  'yellow-l)  palette)
     :green-aux-fg              (solarized-get-color (if light? 'green-d   'green-l)   palette)
     :cyan-aux-fg               (solarized-get-color (if light? 'cyan-d    'cyan-l)    palette)
     :blue-aux-fg               (solarized-get-color (if light? 'blue-d    'blue-l)    palette)
     :violet-aux-fg             (solarized-get-color (if light? 'violet-d  'violet-l)  palette)
     :magenta-aux-fg            (solarized-get-color (if light? 'magenta-d 'magenta-l) palette)

     :red-aux-bg                (solarized-get-color (if light? 'red-l     'red-d)     palette)
     :orange-aux-bg             (solarized-get-color (if light? 'orange-l  'orange-d)  palette)
     :yellow-aux-bg             (solarized-get-color (if light? 'yellow-l  'yellow-d)  palette)
     :green-aux-bg              (solarized-get-color (if light? 'green-l   'green-d)   palette)
     :cyan-aux-bg               (solarized-get-color (if light? 'cyan-l    'cyan-d)    palette)
     :blue-aux-bg               (solarized-get-color (if light? 'blue-l    'blue-d)    palette)
     :violet-aux-bg             (solarized-get-color (if light? 'violet-l  'violet-d)  palette)
     :magenta-aux-bg            (solarized-get-color (if light? 'magenta-l 'magenta-d) palette)

     :red-aux-aux-fg            (solarized-get-color 'red-1fg     palette)
     :orange-aux-aux-fg         (solarized-get-color 'orange-1fg  palette)
     :yellow-aux-aux-fg         (solarized-get-color 'yellow-1fg  palette)
     :green-aux-aux-fg          (solarized-get-color 'green-1fg   palette)
     :cyan-aux-aux-fg           (solarized-get-color 'cyan-1fg    palette)
     :blue-aux-aux-fg           (solarized-get-color 'blue-1fg    palette)
     :violet-aux-aux-fg         (solarized-get-color 'violet-1fg  palette)
     :magenta-aux-aux-fg        (solarized-get-color 'magenta-1fg palette)

     :red-aux-aux-bg            (solarized-get-color 'red-1bg     palette)
     :orange-aux-aux-bg         (solarized-get-color 'orange-1bg  palette)
     :yellow-aux-aux-bg         (solarized-get-color 'yellow-1bg  palette)
     :green-aux-aux-bg          (solarized-get-color 'green-1bg   palette)
     :cyan-aux-aux-bg           (solarized-get-color 'cyan-1bg    palette)
